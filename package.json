  "description": "A graphql service to handle incoming requests from the apollo-clientt, it's purpose is to find movies, shows, actors and provide various filtering capabilites",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node index.js",
    "server": "nodemon index.js",
